    while true do
        if config.loopDoors then
            for i,v in next, Nitrix.Doors do 
                Nitrix.OpenDoor(v)
            end
        end
        wait(3)
    end
end)()

function UnLoadWorld()
    if game.Players.localPlayer.PlayerScripts.WorldUnload:FindFirstChild("WorldUnload") then
    game.Players.localPlayer.PlayerScripts.WorldUnload.WorldUnload:Destroy()
    end
    if game.Players.localPlayer.PlayerScripts.WorldUnload:FindFirstChild("HumanoidUnload") then
        game.Players.localPlayer.PlayerScripts.HumanoidUnload.HumanoidUnload:Destroy()
    end
end

if config.antiUnLoad then
    UnLoadWorld()
end



game.Workspace.DescendantAdded:Connect(function(part)
    wait(0.1)
    if config.antiLasers then
        if part.Name == "TouchInterest" then
            if part.Parent then
                if part.Parent.Name ~= "LaserTouch" and part.Parent.Name ~= "TriggerDoor" and part.Parent.Name ~= "TouchToEnter" and part.Parent.Name ~= "HatchTouch" and part.Parent.Name ~= "TriggerFoundBossRoom" and part.Parent.Parent.Name ~= "ExitDoor" then
                    part.Parent:GetAttributeChangedSignal("CanTouch"):Connect(function()
                        part.Parent.CanTouch = false
                    end)
                    part.Parent.CanTouch = false
                end
            end
        end
    end
    if config.antiBoss then
        if part.Name == "MansionBossNPCDamage" then
            part:Destroy()
        end
    end
    if config.antiDoors then
        if part.Name == "Door" or part.Name == "SwingDoor" or part.Name == "FrontDoorLeft" or part.Name == "FrontDoorRight" or part.Name == "SlideDoor" then
            for k, l in pairs(part:GetDescendants()) do
                if l:IsA("Part") or l:IsA("MeshPart") then
                    l:GetAttributeChangedSignal("CanCollide"):Connect(function()
                        l.CanCollide = false
                    end)
                    l.CanCollide = false
                end
            end
        end
    end
    if config.antiLasers then
        if part.Name == "DartFire" or part.Name == "DartDamage" then
            part:Destroy()
        end
    end
    if config.antiNPC then
        if part.Name == "NPCDestObj" then
            part:Destroy()
        end
    end
    if part.Name == "Drop" then
        if config.airDropESPEnabled then
            CreateEsp(part)
        end
    end
end)

function antiStuff()
    for k, part in pairs(game.Workspace:GetChildren()) do
        if config.antiLasers then
            if part.Name == "TouchInterest" then
                if part.Parent then
                    if part.Parent.Name ~= "LaserTouch" and part.Parent.Name ~= "TriggerDoor" and part.Parent.Name ~= "TouchToEnter" and part.Parent.Name ~= "HatchTouch" and part.Parent.Name ~= "TriggerFoundBossRoom" and part.Parent.Parent.Name ~= "ExitDoor" then
                        part.Parent:GetAttributeChangedSignal("CanTouch"):Connect(function()
                            part.Parent.CanTouch = false
                        end)
                        part.Parent.CanTouch = false
                    end
                end
            end
        end
        if config.antiBoss then
            if part.Name == "MansionBossNPCDamage" then
                part:Destroy()
            end
        end
        if config.antiDoors then
            if part.Name == "Door" or part.Name == "SwingDoor" or part.Name == "FrontDoorLeft" or part.Name == "FrontDoorRight" or part.Name == "SlideDoor" then
                for k, l in pairs(part:GetDescendants()) do
                    if l:IsA("Part") or l:IsA("MeshPart") then
                        l:GetAttributeChangedSignal("CanCollide"):Connect(function()
                            l.CanCollide = false
                        end)
                        l.CanCollide = false
                    end
                end
            end
        end
        if config.antiLasers then
            if part.Name == "DartFire" or part.Name == "DartDamage" then
                part:Destroy()
            end
        end
        if config.antiNPC then
            if part.Name == "NPCDestObj" then
                part:Destroy()
            end
        end
    end
end

antiStuff()

game:GetService("UserInputService").JumpRequest:connect(function()
	if config.infiniteJump then
		Player.Character.Humanoid:ChangeState("Jumping")
	end
end)

local shooting = false
coroutine.wrap(function()
    while true do
        local shoot = false
        if config.killAuraEnabled then
            for k, v in pairs(game.Players:GetChildren()) do
                if v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                    if (v.Character.HumanoidRootPart.Position - Player.Character.HumanoidRootPart.Position).magnitude < config.killAuraDistance and v.Team ~= Player.Team then
                        if not (Player.Team == "Criminal" and v.Team == "Prisoner") or not (Player.Team == "Prisoner" and v.Team == "Criminal") then
                            shoot = true
                        end
                    end
                end
            end

            if shoot then
                if not shooting then
                    network:FireServer(hash, "AK47")
                
                    wait(1)

                    pcall(function()
                        toolname = "AK47"
                        if Player.Backpack:FindFirstChild(toolname) and Player.Character:FindFirstChild(toolname) == nil then
                            local tool = Player.Backpack:FindFirstChild(toolname)
                            Player.Character.Humanoid:EquipTool(tool)
                        end
                    end)

                    local args = {
                        [1] = true
                    }

                    game:GetService("Players").LocalPlayer.Folder.AK47.InventoryEquipRemote:FireServer(unpack(args))

                    wait(0.5)
                end

                shooting = true
                game:GetService('VirtualInputManager'):SendMouseButtonEvent(0,0,0,true,game,0)
            else
                if shooting then
                    game:GetService('VirtualInputManager'):SendMouseButtonEvent(0,0,0,false,game,0)
                    shooting = false
                end
            end
        end
        wait(0.5)
    end
end)()


coroutine.wrap(function()
    while true do
        while config.dropGuns do
            for k, v in pairs(Player.Items:GetChildren()) do
                if v.Name ~= "MansionInvite" and v.Name ~= "Cuffed" and v.Name ~= "HandCuffs" and v.Name ~= "Taser" then
                    network:FireServer(gunHash, v.Name)
                    wait(0.1)
                    EquipGun(v.Name)
                    wait(0.1)
                    network:FireServer(dropHash)
                    wait(0.1)
                end
            end
            wait()
        end
        wait(0.1)
    end
end)()

local speed = false

function ApplySpeed(value, input)
    if config.speedEnabled then
        if input == config.speedKey then
            if Player.Character then
                Player.Character.Humanoid.WalkSpeed = value
            end
        end
    end
end

Player.CharacterAdded:Connect(function(char)
    repeat wait() until char:FindFirstChild("Humanoid")
    char.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
        if speed and config.speedEnabled then
            Player.Character.Humanoid.WalkSpeed = config.speedValue
        end
    end)
end)

Player.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
    if speed and config.speedEnabled then
        Player.Character.Humanoid.WalkSpeed = config.speedValue
    end
end)

game:GetService("UserInputService").InputBegan:connect(function(input)
    if input.KeyCode == config.speedKey then
        ApplySpeed(config.speedValue, input.KeyCode)
        speed = true
    end
end)

game:GetService("UserInputService").InputEnded:connect(function(input)
    if input.KeyCode == config.speedKey then
        ApplySpeed(16, input.KeyCode)
        speed = false
    end
end)

local maxdistance = 100000000000000 --using math.huge makes this script wonky
local mouse = Player:GetMouse()
function GetVelocity(pos1,pos2,StudsPerSecond)
    local distance = (pos2 - pos1)
    local mag = distance.Magnitude
    return (distance/mag)*StudsPerSecond
end
function getkey(keycode)
    local key = tostring(keycode):lower()
    local findcode, a = key:find("keycode.")
    return key:sub(a+1)
end
local keys = {}
coroutine.wrap(function()
    while true do
        local engine = nil
        for i,v in pairs(game:GetService("Workspace").Vehicles:GetChildren()) do
            if pcall(function() return v.Engine and v.Seat.PlayerName end) then
                if v.Seat.PlayerName.Value == Player.Name then
                    engine = v.Engine
                    break
                end
            end
        end
        if not Player.Character then 
            flying = false
        end
        if Player.Character and Player.Character.Humanoid.Health == 0 then
            flying = false
            Player.Character.Humanoid.CameraOffset = Vector3.new(0,0,0)
        end
        if Player.Character and engine == nil then
            flying = false
            Player.Character.Humanoid.CameraOffset = Vector3.new(0,0,0)
        end
        local frontoffset = CFrame.new() + Vector3.new(0,0,-maxdistance)
        local backoffset = CFrame.new() + Vector3.new(0,0,maxdistance)
        local leftoffset = CFrame.new() + Vector3.new(-maxdistance,0,0)
        local rightoffset = CFrame.new() + Vector3.new(maxdistance,0,0)
        local velocity = Vector3.new()
        if flying then
            if keys.w_active then
                velocity = velocity + GetVelocity(engine.Position,(engine.CFrame*frontoffset).Position,config.vehicleFlySpeed)
            end
            if keys.s_active then
                velocity = velocity + GetVelocity(engine.Position,(engine.CFrame*backoffset).Position,config.vehicleFlySpeed)
            end
            if keys.a_active then
                velocity = velocity + GetVelocity(engine.Position,(engine.CFrame*leftoffset).Position,config.vehicleFlySpeed)
            end
            if keys.d_active then
                velocity = velocity + GetVelocity(engine.Position,(engine.CFrame*rightoffset).Position,config.vehicleFlySpeed)
            end
            engine.Velocity = velocity
            engine.CFrame = CFrame.new(engine.Position, (game:GetService("Workspace").Camera.CFrame*frontoffset).Position)
        end
        if flying and not keys.w_active and not keys.a_active and not keys.s_active and not keys.d_active then
            engine.Velocity = Vector3.new(0,1.005,0)
        end
        wait()
    end
end)()


game:GetService("UserInputService").InputBegan:connect(function(key,processed)
    if processed then return end
    if game:GetService("UserInputService"):IsKeyDown(config.vehicleFlyKey) then
        if config.vehicleFlyEnabled then
            flying = not flying
            if flying then
                Player.Character.Humanoid.CameraOffset = Vector3.new(2,0,0)
            else
                Player.Character.Humanoid.CameraOffset = Vector3.new(0,0,0)
            end
        else
            flying = false
            Player.Character.Humanoid.CameraOffset = Vector3.new(0,0,0)
        end
    end
    keys[getkey(key.KeyCode).."_active"] = true
end)
game:GetService("UserInputService").InputEnded:connect(function(key)
    keys[getkey(key.KeyCode).."_active"] = false
end)

local dir = 0

coroutine.wrap(function()
    while true do
        for i,v in next,workspace.Vehicles:children() do
            if v.Name ~= "Heli" then
                if v:FindFirstChild("Engine") then
                    if config.vehicleSpeedEnabled then
                        local bodyForce,lookVecotr = v.Engine:FindFirstChild("NewForce"), v:GetPrimaryPartCFrame().lookVector
                        if not bodyForce then
                            bodyForce = v.Engine:FindFirstChild("BodyForce") or v.Engine:FindFirstChild("VectorForce")
                            if bodyForce then
                                bodyForce = bodyForce:Clone()
                                bodyForce.Parent = v.Engine
                                bodyForce.Name = "NewForce"
                            end
                        end
                        if bodyForce then
                            speed = config.vehicleSpeed * -1000
                            local multiplier = 1
                            if v.Name:lower():find("bike") then multiplier = .3 elseif v.Name:lower():find("bugg") then multiplier = .5 end
                            if v.Seat.PlayerName.Value ~= game:GetService("Players").LocalPlayer.Name then fa = 0 end
                            bodyForce.Force = Vector3.new(lookVecotr.X*speed*dir*multiplier,0,lookVecotr.Z*speed*dir*multiplier)
                        end
                    else
                        if v.Engine:FindFirstChild("NewForce") then
                            v.Engine:FindFirstChild("NewForce"):Destroy()
                        end
                    end
                end
            end
        end
        wait()
    end
end)()
val = {
    {Enum.KeyCode.W,1};
    {Enum.KeyCode.S,-1};
}

function check(InputKey)
   for i,v in next,val do
       if v[1] == InputKey.KeyCode then
           return v[2]
       end
   end
end

game:GetService("UserInputService").InputBegan:connect(function(input)
    dir = check(input) or dir
end)

game:GetService("UserInputService").InputEnded:connect(function(input)
    if check(input) == dir then
        dir = 0
    end
end)

function CreateEsp(TargetPlayer)
    local Name = Drawing.new("Text")
    local Health = Drawing.new("Text")
    game:GetService("RunService").RenderStepped:Connect(function()
        if Name == nil then
            return
        end
        if TargetPlayer.Name ~= "Drop" then
            if TargetPlayer and TargetPlayer.Character ~= nil and TargetPlayer.Character:FindFirstChild("Humanoid") ~= nil and TargetPlayer.Character:FindFirstChild("HumanoidRootPart") ~= nil and TargetPlayer.Character.Humanoid.Health > 0 and TargetPlayer.Character:FindFirstChild("Head") ~= nil then
                if (config.prisonerESPEnabled and TargetPlayer.Team.Name == "Prisoner") or (config.criminalESPEnabled and TargetPlayer.Team.Name == "Criminal") or (config.policeESPEnabled and TargetPlayer.Team.Name == "Police") then
                    if config.nameESPEnabled then
                        local Target2dPosition,IsVisible = game:GetService("Workspace").CurrentCamera:WorldToViewportPoint(TargetPlayer.Character.HumanoidRootPart.Position)
                        local scale_factor = 1 / (Target2dPosition.Z * math.tan(math.rad(game:GetService("Workspace").CurrentCamera.FieldOfView * 0.5)) * 2) * 100
                        local width, height = math.floor(40 * scale_factor), math.floor(60 * scale_factor)
                        Name.Visible = IsVisible
                        Name.Color = TargetPlayer.TeamColor.Color
                        Name.Text = TargetPlayer.DisplayName.." "..math.floor((game:GetService("Workspace").CurrentCamera.CFrame.p - TargetPlayer.Character.HumanoidRootPart.Position).magnitude).."m"
                        Name.Center = true
                        Name.Outline = false
                        Name.Position = Vector2.new(Target2dPosition.X,Target2dPosition.Y - height * 0.5 + -15)
                        Name.Font = 2
                        Name.Size = config.espTextSize
                    else
                        Name.Visible = false
                    end
                    if config.healthESPEnabled then
                        local Target2dPosition,IsVisible = game:GetService("Workspace").CurrentCamera:WorldToViewportPoint(TargetPlayer.Character.HumanoidRootPart.Position)
                        local scale_factor = 1 / (Target2dPosition.Z * math.tan(math.rad(game:GetService("Workspace").CurrentCamera.FieldOfView * 0.5)) * 2) * 100
                        local width, height = math.floor(40 * scale_factor), math.floor(60 * scale_factor)
                        Health.Visible = IsVisible
                        Health.Color = TargetPlayer.TeamColor.Color
                        Health.Text = "HP: "..TargetPlayer.Character.Humanoid.Health
                        Health.Center = true
                        Health.Outline = false
                        Health.Position = Vector2.new(Target2dPosition.X,Target2dPosition.Y - height * 0.5 + -10)
                        Health.Font = 2
                        Health.Size = config.espTextSize - 2
                    else
                        Health.Visible = false
                    end
                else
                    Name.Visible = false
                    Health.Visible = false
                end
            else
                Name.Visible = false
                Health.Visible = false
            end
        else
            if config.nameESPEnabled then
                local Target2dPosition,IsVisible = game:GetService("Workspace").CurrentCamera:WorldToViewportPoint(TargetPlayer.Root.Position)
                local scale_factor = 1 / (Target2dPosition.Z * math.tan(math.rad(game:GetService("Workspace").CurrentCamera.FieldOfView * 0.5)) * 2) * 100
                local width, height = math.floor(40 * scale_factor), math.floor(60 * scale_factor)
                Name.Visible = IsVisible
                Name.Color = TargetPlayer.TeamColor.Color
                Name.Text = "AirDrop "..math.floor((game:GetService("Workspace").CurrentCamera.CFrame.p - TargetPlayer.Root.Position).magnitude).."m"
                Name.Center = true
                Name.Outline = false
                Name.Position = Vector2.new(Target2dPosition.X,Target2dPosition.Y - height * 0.5 + -15)
                Name.Font = 2
                Name.Size = 15
            else
                Name.Visible = false
            end
        end
    end)
end

for _,v in pairs(game:GetService("Players"):GetPlayers()) do
    if v ~= game:GetService("Players").LocalPlayer then
        CreateEsp(v)
        v.CharacterAdded:Connect(CreateEsp(v))
    end
end

game:GetService("Players").PlayerAdded:Connect(function(v)
    if v ~= game:GetService("Players").LocalPlayer then
        CreateEsp(v)
        v.CharacterAdded:Connect(CreateEsp(v))
    end
end)

function BodyESP(TargetPlayer)
    if TargetPlayer ~= Player then
        if TargetPlayer and TargetPlayer.Character and TargetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            if config.bodyESPEnabled then
                if (config.prisonerESPEnabled and TargetPlayer.Team.Name == "Prisoner") or (config.crimanalESPEnabled and TargetPlayer.Team.Name == "Criminal") or (config.policeESPEnabled and TargetPlayer.Team.Name == "Police") then
                    if not TargetPlayer.Character.HumanoidRootPart:FindFirstChild("Highlight") then
                        local highlight = Instance.new("Highlight")
                        highlight.Adornee = TargetPlayer.Character
                        highlight.Parent = TargetPlayer.Character.HumanoidRootPart
                        highlight.Name = "Highlight"
                        highlight.FillColor = TargetPlayer.TeamColor.Color
                        highlight.OutlineColor = TargetPlayer.TeamColor.Color
                    else
                        local highlight = TargetPlayer.Character.HumanoidRootPart:FindFirstChild("Highlight")
                        highlight.FillColor = TargetPlayer.TeamColor.Color
                        highlight.OutlineColor = TargetPlayer.TeamColor.Color
                    end
                else
                    if TargetPlayer.Character.HumanoidRootPart:FindFirstChild("Highlight") then
                        TargetPlayer.Character.HumanoidRootPart:FindFirstChild("Highlight"):Destroy()
                    end
                end
            else
                if TargetPlayer.Character.HumanoidRootPart:FindFirstChild("Highlight") then
                    TargetPlayer.Character.HumanoidRootPart:FindFirstChild("Highlight"):Destroy()
                end
            end
        end
    end
end


for k, v in pairs(game:GetService("Players"):GetChildren()) do
    repeat wait() until v.Character
    BodyESP(v)
    v.CharacterAdded:Connect(function()
        BodyESP(v)
    end)
    v:GetPropertyChangedSignal("Team"):Connect(function()
        BodyESP(player)
    end)
end

game:GetService("Players").PlayerAdded:Connect(function(player)
    repeat wait() until player.Character
    BodyESP(player)
    player.CharacterAdded:Connect(function()
        BodyESP(player)
    end)
    player:GetPropertyChangedSignal("Team"):Connect(function()
        BodyESP(player)
    end)
end)

for k, v in pairs(game.Workspace:GetDescendants()) do
    if v.Name == "Drop" then
        if config.airDropESPEnabled then
            CreateEsp(v)
        end
    end
end

coroutine.wrap(function()
    while true do
        local Heli
        for i,v in pairs(workspace.Vehicles:GetChildren()) do
            if pcall(function() return v.Seat.PlayerName end) then
                if v.Seat.PlayerName.Value == Player.Name then
                    Heli = v
                    break
                end
            end
        end
        if config.modifyRopeLength then
            if Heli ~= nil then
                Heli.Winch.RopeConstraint.Length = config.ropeLength
            end
        else
            if Heli ~= nil then
                Heli.Winch.RopeConstraint.Length = 30
            end
        end
        wait()
    end
end)()

local mouse = game:GetService('Players').LocalPlayer:GetMouse()
local function hotkeyHandler(key)
    if not config.infiniteNitroEnabled then
        if (key == 'q') then
            for _, v in next, getgc(true) do
                if (type(v) == 'table' and rawget(v, 'Nitro')) then
                    v.Nitro = math.huge --250 Default
                    wait()
                end
            end
        end
    end
end
mouse.KeyDown:connect(hotkeyHandler)

function editGuns()
    for k, v in pairs(game.ReplicatedStorage.Game.ItemConfig:GetChildren()) do
        if config.autoFire then
            if require(v).FireAuto ~= nil then
                require(v).FireAuto = true
            end
        end
        if config.antiRecoil then 
            if require(v).CamShakeMagnitude ~= nil then
                require(v).CamShakeMagnitude = 0
            end
            if require(v).FireAutoMobile ~= nil then
                require(v).FireAutoMobile = false
            end
        end
    end
end

editGuns()

local Heli
coroutine.wrap(function()
    while true do
        if heli == nil then
            for i,v in pairs(workspace.Vehicles:GetChildren()) do
                if pcall(function() return v.Seat.PlayerName end) then
                    if v.Seat.PlayerName.Value == Player.Name then
                        Heli = v
                    end
                end
            end
        end
        if Heli and Heli.Seat.PlayerName.Value ~= Player.Name then
            Heli = nil
        end
        if config.instaConnect then
            if Heli and Heli.Preset:FindFirstChild("RopePull") then
                for k, v in pairs(game.Workspace.Vehicles:GetChildren()) do
                    if v ~= Heli and v:FindFirstChild("Engine") then 
                        if (Heli.Engine.Position - v.Engine.Position).magnitude < config.connectDistance then
                            Heli.Preset.RopePull.ReqLink:FireServer(v, Vector3.new(0, 0, 0))
                        end
                    end
                end
            end
        end
        wait()
    end
end)()
