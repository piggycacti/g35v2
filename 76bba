local ToggleMenuButtonHold = false

game:GetService("UserInputService").InputBegan:Connect(function(input, proccesed)
    if not ToggleMenuButtonHold then
        if input.KeyCode == config.toggleMenuKey then
            if not pxstaHack.BackGround.Visible then
                pxstaHack.BackGround.Visible = true
            else
                pxstaHack.BackGround.Visible = false
            end
        end
    else
        config.toggleMenuKey = input.KeyCode
        pxstaHack.MenuToggleKey.Text = string.upper(input.KeyCode.Name)
        ToggleMenuButtonHold = false
    end
end)

pxstaHack.MenuToggleKey.MouseButton1Click:Connect(function()
    ToggleMenuButtonHold = true
    pxstaHack.MenuToggleKey.Text = ""
end)



pxstaHack.BackGround.Active = true
pxstaHack.BackGround.Draggable = true

pxstaHack.PlayerTab.Visible = true
pxstaHack.CombatTab.Visible = false
pxstaHack.RobberyTab.Visible = false
pxstaHack.VehicleTab.Visible = false
pxstaHack.VisualTab.Visible = false
pxstaHack.SettingsTab.Visible = false

