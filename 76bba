pxstaHack.KillAuraSlider:FindFirstChild("Handle").Position = UDim2.new(0, (config.killAuraDistance - 50) / 2.75, pxstaHack.KillAuraSlider:FindFirstChild("Handle").Position.Y.Scale, pxstaHack.KillAuraSlider:FindFirstChild("Handle").Position.Y.Offset)
pxstaHack.SpeedSlider:FindFirstChild("Handle").Position = UDim2.new(0, (config.speedValue - 16) / 0.92, pxstaHack.SpeedSlider:FindFirstChild("Handle").Position.Y.Scale, pxstaHack.SpeedSlider:FindFirstChild("Handle").Position.Y.Offset)
pxstaHack.VehicleFlySpeedSlider:FindFirstChild("Handle").Position = UDim2.new(0, (config.vehicleFlySpeed - 50) / 2.75, pxstaHack.VehicleFlySpeedSlider:FindFirstChild("Handle").Position.Y.Scale, pxstaHack.VehicleFlySpeedSlider:FindFirstChild("Handle").Position.Y.Offset)
pxstaHack.VehicleSpeedSlider:FindFirstChild("Handle").Position = UDim2.new(0, (config.vehicleSpeed - 50) / 2.75, pxstaHack.VehicleSpeedSlider:FindFirstChild("Handle").Position.Y.Scale, pxstaHack.VehicleSpeedSlider:FindFirstChild("Handle").Position.Y.Offset)
pxstaHack.RopeLengthSlider:FindFirstChild("Handle").Position = UDim2.new(0, (config.ropeLength - 10) / 0.95, pxstaHack.RopeLengthSlider:FindFirstChild("Handle").Position.Y.Scale, pxstaHack.RopeLengthSlider:FindFirstChild("Handle").Position.Y.Offset)
pxstaHack.InstaConnectDistanceSlider:FindFirstChild("Handle").Position = UDim2.new(0, (config.connectDistance - 20) / 1.4, pxstaHack.InstaConnectDistanceSlider:FindFirstChild("Handle").Position.Y.Scale, pxstaHack.InstaConnectDistanceSlider:FindFirstChild("Handle").Position.Y.Offset)
pxstaHack.ESPTextSizeSlider:FindFirstChild("Handle").Position = UDim2.new(0, (config.espTextSize - 10) / 0.1, pxstaHack.ESPTextSizeSlider:FindFirstChild("Handle").Position.Y.Scale, pxstaHack.ESPTextSizeSlider:FindFirstChild("Handle").Position.Y.Offset)


coroutine.wrap(function()
    while true do
        config.killAuraDistance = pxstaHack.KillAuraSlider:FindFirstChild("Handle").Position.X.Offset * 2.75 + 50
        pxstaHack.KillAuraValue.Text = math.round(config.killAuraDistance * 100) / 100
        config.speedValue = pxstaHack.SpeedSlider:FindFirstChild("Handle").Position.X.Offset * 0.92 + 16
        pxstaHack.SpeedValue.Text = math.round(config.speedValue * 100) / 100
        config.vehicleFlySpeed = pxstaHack.VehicleFlySpeedSlider:FindFirstChild("Handle").Position.X.Offset * 2.75 + 50
        pxstaHack.VehicleFlySpeedValue.Text = math.round(config.vehicleFlySpeed * 100) / 100
        config.vehicleSpeed = pxstaHack.VehicleSpeedSlider:FindFirstChild("Handle").Position.X.Offset * 2.75 + 50
        pxstaHack.VehicleSpeedValue.Text = math.round(config.vehicleSpeed * 100) / 100
        config.ropeLength = pxstaHack.RopeLengthSlider:FindFirstChild("Handle").Position.X.Offset * 0.95 + 10
        pxstaHack.RopeLengthValue.Text = math.round(config.ropeLength * 100) / 100
        config.connectDistance = pxstaHack.InstaConnectDistanceSlider:FindFirstChild("Handle").Position.X.Offset * 1.4 + 20
        pxstaHack.InstaConnectDistanceValue.Text = math.round(config.connectDistance * 100) / 100
        config.espTextSize = pxstaHack.ESPTextSizeSlider:FindFirstChild("Handle").Position.X.Offset * 0.1 + 10
        pxstaHack.ESPTextSizeValue.Text = math.round(config.espTextSize * 100) / 100
        wait()
    end
end)()


-----------------------------------------------------------------------------

function load()
    local HttpService = game:GetService("HttpService")
    if readfile and isfile then 
        if isfile("pxstaHack.txt") then
            config = HttpService:JSONDecode(readfile("pxstaHack.txt"))
        end
    end
end

function save()
    local json
    local HttpService = game:GetService("HttpService")
    if writefile then 
        json = HttpService:JSONEncode(config)
        writefile("pxstaHack.txt", json)
    end
end

--load()

coroutine.wrap(function()
    while true do
        --save()
        wait(5)
    end
end)()

-----------------------------------------------------------------------------

function EquipGun(Name)

    toolname = Name
    if Bot.Backpack:FindFirstChild(toolname) and Bot.Character:FindFirstChild(toolname) == nil then
        local tool = Bot.Backpack:FindFirstChild(toolname)
        Bot.Character.Humanoid:EquipTool(tool)
    end

    local args = {
        [1] = true
    }
    if Player.Folder:FindFirstChild(Name) then
        Player.Folder:FindFirstChild(Name).InventoryEquipRemote:FireServer(unpack(args))
    end
end

if config.antiRagdoll then
    require(game:GetService("ReplicatedStorage").Game.Paraglide).IsFlying = function()
        return tostring(getfenv(2).script) == "Falling"
    end
end

local UI = require(game:GetService("ReplicatedStorage").Module:WaitForChild("UI"));

coroutine.wrap(function()
    while true do
        if config.instantCircle then
            for k, v in pairs(UI.CircleAction.Specs) do
                if v.Name ~= "Rob" and v.Name ~= "Open Crate" then
                    v.Duration = 0;
                    v.Timed = true;
                end
            end
        end
        wait()
    end
end)()


local old = old or require(game:GetService("ReplicatedStorage").Module.RayCast).RayIgnoreNonCollideWithIgnoreList

function SilentAim()
    if config.silentAim then
        require(game:GetService("ReplicatedStorage").Module.RayCast).RayIgnoreNonCollideWithIgnoreList = function(...)
            local nearestDistance, nearestEnemy = 999999, nil
            local nearestEnemyPlayer
            for i,v in pairs(game:GetService("Players"):GetPlayers()) do
                if v.Team ~= game:GetService("Players").LocalPlayer.Team and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                    if v.Team.Name ~= "Prisoner" then
                        if v.Character.Humanoid.Health > 0 then
                            if (v.Character.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < nearestDistance then
                                nearestDistance, nearestEnemy = (v.Character.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude, v
                                nearestEnemyPlayer = true
                            end
                        end
                    end
                end
            end
            for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
                if v.Name == "Drop" then
                    for k, j in pairs(v:GetDescendants()) do
                        if j:FindFirstChild("Humanoid") and j:FindFirstChild("HumanoidRootPart") then
                            if j.Humanoid.Health > 0 then
                                if (j.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < nearestDistance then
                                    nearestDistance, nearestEnemy = (j.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude, j
                                    nearestEnemyPlayer = false
                                end
                            end
                        end
                    end
                end
                if v.Name == "MansionRobbery" then
                    for k, j in pairs(v:GetDescendants()) do
                        if j:FindFirstChild("Humanoid") and j:FindFirstChild("HumanoidRootPart") then
                            if j.Humanoid.Health > 0 then
                                if (j.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < nearestDistance then
                                    nearestDistance, nearestEnemy = (j.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude, j
                                    nearestEnemyPlayer = false
                                end
                            end
                        end
                    end
                end
            end
            local arg = {old(...)}
            if (tostring(getfenv(2).script) == "BulletEmitter" or tostring(getfenv(2).script) == "Taser") and nearestEnemy then
                if nearestEnemyPlayer then
                    arg[1] = nearestEnemy.Character.HumanoidRootPart
                    arg[2] = nearestEnemy.Character.HumanoidRootPart.Position
                else
                    arg[1] = nearestEnemy.Head
                    arg[2] = nearestEnemy.Head.Position
                end
            end
            return unpack(arg)
        end
    else
        require(game:GetService("ReplicatedStorage").Module.RayCast).RayIgnoreNonCollideWithIgnoreList = old
    end
end

