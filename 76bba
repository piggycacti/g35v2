game:service"Players".LocalPlayer.Idled:connect(function()
    if config.antiAFK then
        VirtualUser:CaptureController()VirtualUser:ClickButton2(Vector2.new())
    end
end)


local plrUtils = game:GetService("ReplicatedStorage").Game.PlayerUtils
function GetKey()
    require(plrUtils).hasKey = function() 
        return true
    end
end

function Respawn()
    if config.autoKey then
        GetKey()
    end
    if config.getGuns then
        for k, v in pairs(Player.Items:GetChildren()) do
            network:FireServer(gunHash, v.Name)
        end
    end
end

coroutine.wrap(function()
    while true do
        if Player.Character.Humanoid and Player.Character.Humanoid.Health == 0 then
            repeat wait() until Player.Character.Humanoid and Player.Character.Humanoid.Health ~= 0
            Respawn()
        end
        wait(1)
    end
end)()

