    antiRagdoll = false,
    loopDoors = false,
    infiniteJump = false,
    speedEnabled = false,
    speedValue = 200,
    speedKey = Enum.KeyCode.LeftShift,
    instantCircle = false,
    antiLasers = false,
    antiBoss = false,
    antiDoors = false,
    antiNPC = false,
    unLoad = false,
    dropGuns = false,
    vehicleSpeed = 600,
    vehicleSpeedEnabled = false,
    vehicleFlyEnabled = false,
    vehicleFlyKey = Enum.KeyCode.LeftAlt,
    vehicleFlySpeed = 600,
    nameESPEnabled = false,
    healthESPEnabled = false,
    bodyESPEnabled = false,
    prisonerESPEnabled = false,
    criminalESPEnabled = false,
    policeESPEnabled = false,
    airDropESPEnabled = false,
    modifyRopeLength = false,
    ropeLength = 30,
