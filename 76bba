pxstaHack.GetGunTextBox.FocusLost:Connect(function(enterPressed, _inputObject)
    local GunName = pxstaHack.GetGunTextBox.Text
    pxstaHack.GetGunTextBox.Text = "GUN NAME"
	if enterPressed then
        network:FireServer(gunHash, GunName)
	end
end)

local SpeedKeyButtonHold = false

game:GetService("UserInputService").InputBegan:Connect(function(input, proccesed)
    if SpeedKeyButtonHold then
        config.speedKey = input.KeyCode
        pxstaHack.SpeedKeyInput.Text = string.upper(input.KeyCode.Name)
        SpeedKeyButtonHold = false
    end
end)

