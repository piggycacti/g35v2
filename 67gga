function MethodPrint(Name, Object)
    print("["..Name.."] - "..tostring(Object))
end
local Remote
for _,V in pairs(getgc(true)) do
    if typeof(V) == "table" and rawget(V, "FireServer") and typeof(rawget(V, "FireServer")) == "function" then
        Remote = V
    end
end
local OldFireserver = Remote.FireServer

Remote.FireServer = function(event, ...)
    local Caller = debug.getinfo(2).func
    if #debug.getupvalues(Caller) >= 6 and debug.getupvalue(Caller, 6) == workspace.CurrentCamera then
        return
    end 
    local printRemote = true
    for k, v in pairs({...}) do
        if typeof(v) ~= "string" then
            printRemote = false
        end
    end
    if printRemote then
        print("-----------------------")
        for I,V in pairs({...}) do
            MethodPrint(I,V)
        end
    end
    return OldFireserver(event, ...)
end
